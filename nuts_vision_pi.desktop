[Desktop Entry]
Version=1.0
Type=Application
Name=Nuts Vision Pi
Comment=Détection de composants électroniques (IC detector)
Exec=bash -c 'for loc in "$(dirname "$1")" "$HOME/nuts_vision_pi" /home/pi/nuts_vision_pi; do [ -f "$loc/launch.sh" ] && exec "$loc/launch.sh"; done; echo "[nuts_vision_pi] ERREUR : launch.sh introuvable dans les emplacements cherchés." >&2; exit 1' _ %k
Icon=camera-photo
Terminal=true
Categories=Science;Education;
StartupNotify=true
